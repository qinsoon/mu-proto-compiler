// malloc

.funcdef @entryFunc <int<32> (int<64> int<64> int<64>)> {
  .label %entry:
    %a = PARAM 0
    %b = PARAM 1
    %b = PARAM 2
    BRANCH %exit

  .label %exit:
    RET <int<32>> 0
}

.funcdef @main <int<32> ()> {
  .label %entry:
    %s = NEWSTACK <int<32> (int<64> int<64> int<64>)> @entryFunc (1 2 3)
    %t = NEWTHREAD %s
    BRANCH %wait

  .label %wait:
    BRANCH %wait

  .label %exit:
    RET <int<32>> 0
}