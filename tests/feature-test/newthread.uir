// malloc

.funcdef @entryFunc <int<32> ()> {
  .label %entry:
    BRANCH %entry

  .label %exit:
    RET <int<32>> 0
}

.funcdef @main <int<32> ()> {
  .label %entry:
    %s = NEWSTACK <int<32> ()> @entryFunc ()
    %t = NEWTHREAD %s
    BRANCH %wait

  .label %wait:
    BRANCH %wait

  .label %exit:
    RET <int<32>> 0
}